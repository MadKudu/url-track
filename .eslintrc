{
	"parser": "babel-eslint",
	"ecmaFeatures": {
		"modules": true,
	},
	"env": {
		"mocha": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"globals": {
	},
	"rules": {
		"comma-dangle": [1, "only-multiline"],
		"indent": [1, "tab"],
		"quotes": [1, "single"],
		"linebreak-style": [2, "unix"],
		"no-console": 1,
		"semi": [2, "always"],
		"strict": [2, "safe"],
		"space-before-function-paren": 1,
		"space-in-parens": [1, "never"],
		"space-infix-ops": 1,
		"space-unary-ops": 1,
		"spaced-comment": 1,
		"valid-jsdoc": 1
	}
}



